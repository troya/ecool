<h1>Listing my_favorites</h1>

<table>
  <tr>
    <th>User</th>
    <th>Resource</th>
  </tr>

<% @my_favorites.each do |my_favorite| %>
  <tr>
    <td><%=h my_favorite.user_id %></td>
    <td><%=h my_favorite.resource_id %></td>
    <td><%= link_to 'Show', my_favorite %></td>
    <td><%= link_to 'Edit', edit_my_favorite_path(my_favorite) %></td>
    <td><%= link_to 'Destroy', my_favorite, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New my_favorite', new_my_favorite_path %>