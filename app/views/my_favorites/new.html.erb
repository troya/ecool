<h1>New my_favorite</h1>

<% form_for(@my_favorite) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :resource_id %><br />
    <%= f.text_field :resource_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', my_favorites_path %>